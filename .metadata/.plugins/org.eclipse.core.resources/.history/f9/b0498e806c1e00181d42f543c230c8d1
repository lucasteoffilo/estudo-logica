package controller;

import util.Teclado;

public class Questao01 {

	public static void main(String[] args) {

		int num;
		num = Teclado.lerInt("Informe um número.");
		if (num > 20) { 
		System.out.println("O número divido por 2 é: ");
		} else { 
		System.out.println("O número informado foi: ");
		}
	}

}
