package controller;

public class Questao11 {

	public static void main(String[] args) {

		int b = 2, e = 11, i = 2, aux = b;

		while (i <= e) {
			aux = aux * b;
			i = i + 1;
		}
		System.out.println("A potência de " + b + " elevado a " + e + " é : " + aux);
		System.out.println(Math.pow(b, e)); // Verifica se o resultado está correto

	}

}
