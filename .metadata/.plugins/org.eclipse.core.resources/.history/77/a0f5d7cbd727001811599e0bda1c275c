package controller;

import util.Teclado;

public class Questao08 {

	public static void main(String[] args) {

		double b = 1, e, i = 1, aux = b;
		b = Teclado.lerDouble("Informe o valor da base: ");
		e = Teclado.lerDouble("Informe o valor do expoente: ");
		do {
			aux = aux * b;
			i++;
		} while (i <= e);
	}

}
