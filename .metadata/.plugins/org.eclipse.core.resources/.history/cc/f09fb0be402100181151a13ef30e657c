package controller;

import util.Teclado;

public class Questao10 {

	public static void main(String[] args) {
		int num1, num2;
		num1 = Teclado.lerInt("Informe um número.");
		num2 = Teclado.lerInt("Informe outro número.");
		if (num1 % num2 == 0) {
			System.out.println("O segundo número informado é um divisor do primeiro.");
		} else {
			System.out.println("O segundo número informado NÃO é um divisor do primeiro.");
		}
	}

}
