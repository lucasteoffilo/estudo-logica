package controller;

import util.Teclado;

public class Questao13 {

	public static void main(String[] args) {

		int a, b, c, primeiro, segundo, terceiro;
		a = Teclado.lerInt("Informe um número.");
		b = Teclado.lerInt("Informe outro número.");
		c = Teclado.lerInt("Informe outro número.");
		
		primeiro = a;
		segundo = a;
		terceiro = a;
		
		if (b < primeiro) {
			primeiro = b;
		}
		if (c < primeiro) {
			primeiro = c;
		}
		System.out.println(primeiro);
		
		if (b > primeiro &&  b < c) {
			segundo = b;
		}
		if (c > primeiro && c < b) {
			segundo = c;
		}
		System.out.println(segundo);
		
		if (b > terceiro) {
			terceiro = b;
		}
		if (c > terceiro) {
			terceiro = c;
		}
		System.out.println(terceiro);
	}

}
