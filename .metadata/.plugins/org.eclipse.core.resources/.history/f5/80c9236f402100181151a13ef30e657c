package controller;

import util.Teclado;

public class Questao08 {

	public static void main(String[] args) {

		int num;
		num = Teclado.lerInt("Informe um número.");
		if (num >= 1 && num <= 10) {
			System.out.println("O número esta entre 1 e 10.");
		} else {
			System.out.println("O número não está entre 1 e 10.");
		}
	}

}
