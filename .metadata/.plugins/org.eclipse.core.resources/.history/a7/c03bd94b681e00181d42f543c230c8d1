package controller;

import util.Teclado;

public class OperadorLogicoE {

	public static void main(String[] args) {

		int num;
		num = Teclado.lerInt("Informe um número.");
		if ((num >= 20) && (num <= 90)) {
			System.out.println("O número está entre 20 e 90.");
		} else {
			System.out.println(""
					+ "O número não está entre 20 e 90.");
		}
	}

}
