package controller;

public class Questao13 {

	public static void main(String[] args) {

		int i, n = 3, n2 = 4, n3 = 5, n4 = 6;
		i = 1;
		while (i <= 10) {
			System.out.println(n + " * " + i + " = " + (n * i));
			i = i + 1;
		}
		i = 1;
		while (i <= 10) {
			System.out.println(n2 + " * " + i + " = " + (n2 * i));
			i = i + 1;
		}
		i = 1;
		while (i <= 10) {
			System.out.println(n3 + " * " + i + " = " + (n3 * i));
			i = i + 1;
		}
		i = 1;
		while (i <= 10) {
			System.out.println(n4 + " * " + i + " = " + (n4 * i));
			i = i + 1;
		}
	}

}
