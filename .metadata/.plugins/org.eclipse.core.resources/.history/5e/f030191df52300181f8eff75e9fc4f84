package controller;

public class Questao14 {

	public static void main(String[] args) {

		int n = 6, f = n - 1, aux = 0;
		
		while (f >= 1) {
			
			aux = n * f;
			f = f - 1;
			aux = aux * f;
			f = f - 1;
			aux = aux * f;
			f = f - 1;
			
			System.out.println("O fator de 5 é igual a: " + aux);
		}
		
	}

}
