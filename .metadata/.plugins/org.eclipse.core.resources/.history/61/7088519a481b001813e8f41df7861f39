package controller;

import util.Teclado;

public class questao05 {

	public static void main(String[] args) {

		double num1, num2;
		
		num1 = Teclado.lerDouble("Infome um número");
		num2 = Teclado.lerDouble("Informe outro número");

		System.out.println("A soma dos dois números é: "+(num1+num2));
		System.out.println("A subtração do primeiro pelo segundo número é: "+ (num1 - num2));
		System.out.println("A subtração do segundo pelo primeiro número é: "+ (num2 - num1));
		// Resto da divisão
		System.out.println("O resto da divisão do primeiro pelo segundo número é: "+ (num1%num2)); 
		 
		 
	}

}
